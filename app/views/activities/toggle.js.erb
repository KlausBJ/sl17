<%# opdatér @activity %>
$("#form_act_<%= @activity.id %>").load('<%= j activity_member_path(@activity, @member) %>');
<%# opdatér @activity.conflicts %>
<% @activity.conflicts.each do |conflict| %>
$("#form_act_<%= conflict.id %>").load('<%= j activity_member_path(conflict, @member) %>');
<% end %>
<% @sold_out.each do |sold_out| %>
$("#form_act_<%= sold_out.id %>").load('<%= j activity_member_path(sold_out, @member) %>');
<% end %>
$("#faktura_<%= @invoice.id %>").html("<%= j render partial: 'members/invoice', locals: { :invoice => @invoice } %>")
