<%# opdatér @activity %>
$("#form_act_<%= @activity.id %>").html('<%= escape_javascript(
  "#{ render partial: 'members/member_activity',
  locals: {
    activity: @activity,
    member: @member
  } }" %>');
<%# opdatér @activity.conflicts %>
<% @activity.conflicts.each do |conflict| %>
$("#form_act_<%= conflict.id %>").html('<%= escape_javascript(
  "#{ render partial: 'members/member_activity',
  locals: {
    activity: conflict,
    member: @member
  } }"
) %>');
<% end %>
